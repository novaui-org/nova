{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "build-storybook"
        ]
      }
    }
  },
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*"
    ],
    "production": [
      "default"
    ]
  },
  "targetDefaults": {
    "dev": {
      "dependsOn": [
        "^dev",
        "^build"
      ],
      "inputs": [
        "default"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "preview": {
      "dependsOn": [
        "^preview"
      ]
    },
    "storybook": {
      "dependsOn": [
        "^storybook"
      ]
    },
    "build-storybook": {
      "dependsOn": [
        "^build-storybook"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ]
    }
  }
}
